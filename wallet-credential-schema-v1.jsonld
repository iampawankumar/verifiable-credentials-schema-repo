{
    "@context": {
        "@version": 1.1,
        "@protected": true,
        "id": "@id",
        "type": "@type",
        "VerifiableCredential": "https://www.w3.org/2018/credentials#VerifiableCredential",
        "SelfSignedCredential": {
            "@id": "https://www.w3.org/ns/credentials/examples#SelfSignedCredential",
            "@context": {
                "id": "@id",
                "type": "@type",
                "issuer": {
                    "@id": "https://www.w3.org/2018/credentials#issuer",
                    "@type": "@id"
                },
                "issuanceDate": {
                    "@id": "https://www.w3.org/2018/credentials#issuanceDate",
                    "@type": "http://www.w3.org/2001/XMLSchema#dateTime"
                },
                "credentialSubject": {
                    "@id": "https://www.w3.org/2018/credentials#credentialSubject",
                    "@type": "@id"
                },
                "proof": {
                    "@id": "https://www.w3.org/2018/credentials#proof",
                    "@type": "@id",
                    "@context": {
                        "type": "https://w3id.org/security#proofType",
                        "created": {
                            "@id": "http://purl.org/dc/terms/created",
                            "@type": "http://www.w3.org/2001/XMLSchema#dateTime"
                        },
                        "proofPurpose": {
                            "@id": "https://w3id.org/security#proofPurpose",
                            "@type": "@id"
                        },
                        "verificationMethod": {
                            "@id": "https://w3id.org/security#verificationMethod",
                            "@type": "@id"
                        },
                        "proofValue": "https://w3id.org/security#proofValue"
                    }
                },
                "claims": {
                    "@id": "https://www.w3.org/ns/credentials/examples#claims",
                    "@type": "@id",
                    "@context": {
                        "userID": "https://www.w3.org/ns/credentials/examples#userID"
                    }
                },
                "design": {
                    "@id": "https://www.w3.org/ns/credentials/examples#design",
                    "@type": "@id",
                    "@context": {
                        "name": "https://schema.org/name",
                        "brand": "https://schema.org/brand",
                        "image": "https://schema.org/image",
                        "bgColor": "https://schema.org/color"
                    }
                }
            }
        },
        "WalletCredentials": "https://www.w3.org/ns/credentials/examples#WalletCredentials"
    },
    "id": "https://www.w3.org/ns/credentials/examples#WalletCredentials",
    "type": "SelfSignedCredential",
    "issuer": "https://www.w3.org/ns/credentials#issuer",
    "issuanceDate": "https://www.w3.org/ns/credentials#issuanceDate",
    "credentialSubject": "https://www.w3.org/ns/credentials#credentialSubject",
    "proof": "https://www.w3.org/ns/credentials#proof",
    "claims": "https://www.w3.org/ns/credentials/examples#claims",
    "design": "https://www.w3.org/ns/credentials/examples#design"
}
 
